            <string>///Movement options

// Player inputs
left = -keyboard_check(ord('A'));
right = keyboard_check(ord('D'));
up = -keyboard_check(ord('W'));
down = keyboard_check(ord('S'));


//Variables needed for movement speed
movementSpeed = 3;
horizonmove = left + right;
verticmove = up + down;
hsp = horizonmove * movementSpeed;
vsp = verticmove * movementSpeed;


//Collisions with walls.

//Horizontal Collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

//Vertical Collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

//Animations
if (horizonmove=0 &amp;&amp; verticmove=0)
{
sprite_index = spr_farmer;
}

if (horizonmove&gt;0) 
{
sprite_index = spr_farmer_right;
}

if (horizonmove&lt;0) 
{
sprite_index = spr_farmer_left;
}


if (verticmove&gt;0) 
{
sprite_index = spr_farmer_down;

}

if (verticmove&lt;0) 
{
sprite_index = spr_farmer_up;
}
</string>
